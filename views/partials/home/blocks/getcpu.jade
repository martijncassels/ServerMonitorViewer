.card
	.card-header
		h5 
			i.fas.fa-address-card
			|                   CPU
	div(ng-if='vm.cpustarting',align='center').loading
			i(aria-hidden="true").fas.fa-circle-notch.fa-spin.fa-3x.fa-lg
	div(ng-show="vm.cpu.name=='SequelizeDatabaseError'",style='margin:5px;').alert-danger
		{{vm.cpu.original.message}}
	canvas(id='line',chart-data='vm.cpudata',chart-labels='vm.cpulabels',chart-series='vm.cpuseries',chart-options='vm.options2',height='100').chart.chart-line
