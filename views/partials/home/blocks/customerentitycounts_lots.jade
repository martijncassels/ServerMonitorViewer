.card
	.card-header
		h5 
			i.fas.fa-address-card
			|                   Lots
	div(ng-if='vm.customerentitycountdatastarting',align='center').loading
			i(aria-hidden="true").fas.fa-circle-notch.fa-spin.fa-3x.fa-lg
	div(uib-alert,ng-show="vm.customerentitycountdata.name=='SequelizeDatabaseError'",style='margin:5px;',close="vm.customerentitycountdata.name=null").alert-danger
		{{vm.customerentitycountdata.original.message}}
	div(uib-alert,ng-show="vm.customerentitycountdata.length<1",style='margin:5px;').alert-danger
		p No data!
	canvas(id='line',chart-data='[vm.customerentitycountdata[2],vm.customerentitycountdata[3],vm.customerentitycountdata[4]]',chart-labels='vm.customerentitycountdata[1]',chart-series='[vm.customerentitycountseries[2],vm.customerentitycountseries[3],vm.customerentitycountseries[4]]',chart-options='vm.options2',height='100',chart-dataset-override="[{pointRadius:0},{pointRadius:0},{pointRadius:0}]").chart.chart-line
