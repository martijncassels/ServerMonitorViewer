.card
	.card-header
		h5 
			i.fas.fa-address-card
			|                   Orders
	div(ng-if='vm.customerentitycountdatastarting',align='center').loading
			i(aria-hidden="true").fas.fa-circle-notch.fa-spin.fa-3x.fa-lg
