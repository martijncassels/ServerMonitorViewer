	canvas(id='line',chart-data='[vm.customerentitycountdata[6],vm.customerentitycountdata[8],vm.customerentitycountdata[10],vm.customerentitycountdata[12]]',chart-labels='vm.customerentitycountdata[1]',chart-series='[vm.customerentitycountseries[6],vm.customerentitycountseries[8],vm.customerentitycountseries[10],vm.customerentitycountseries[12]]',chart-options='vm.options2',height='100',chart-dataset-override="[{pointRadius:0},{pointRadius:0},{pointRadius:0},{pointRadius:0}]").chart.chart-line
