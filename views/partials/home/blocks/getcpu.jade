.card
	.card-header
		h5 
			i.fas.fa-address-card
			|                   CPU
	div(ng-if='vm.cpu_starting',align='center').loading
			i(aria-hidden="true").fas.fa-circle-notch.fa-spin.fa-3x.fa-lg
	div(uib-alert,ng-show="vm.cpu_.name=='SequelizeDatabaseError'",style='margin:5px;',close="vm.cpu_.name=null").alert-danger
		{{vm.cpu.original.message}}
	canvas(id='line',chart-data='vm.cpu_chartdata',chart-labels='vm.cpu_chartdata[0]',chart-series='["Timestamp","Value"]',chart-options='vm.options2',chart-dataset-override="[{pointRadius:0}]",height='100').chart.chart-line
